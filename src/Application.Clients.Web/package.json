{
  "name": "web-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "cap:sync": "cap sync",
    "cap:copy": "cap copy",
    "cap:build": "npm run build && cap sync",
    "cap:android": "npm run cap:build && cap open android",
    "cap:ios": "npm run cap:build && cap open ios",
    "cap:run:android": "npm run cap:build && cap run android",
    "cap:run:ios": "npm run cap:build && cap run ios",
    "cap:build:android": "npm run cap:build && cap build android",
    "cap:build:ios": "npm run cap:build && cap build ios"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.4.0",
    "@capacitor/android": "^7.4.1",
    "@capacitor/app": "^7.0.1",
    "@capacitor/camera": "^7.0.1",
    "@capacitor/cli": "^7.4.1",
    "@capacitor/clipboard": "^7.0.1",
    "@capacitor/core": "^7.4.1",
    "@capacitor/device": "^7.0.1",
    "@capacitor/geolocation": "^7.1.3",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.1",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/network": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@capacitor/toast": "^7.0.1",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@vueuse/core": "^13.4.0",
    "cron-parser": "^5.3.0",
    "cronstrue": "^2.61.0",
    "date-fns": "^4.1.0",
    "lucide-vue-next": "^0.523.0",
    "pinia": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.2.1",
    "eslint": "^9.22.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "sharp": "^0.34.2",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8",
    "workbox-window": "^7.3.0"
  }
}
